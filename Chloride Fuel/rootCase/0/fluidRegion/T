FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "20000";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 900; // default is 900

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    topwall
    {
        type            zeroGradient;
    }
    bottomwall
    {
        type            zeroGradient;
    }
    reflector
    {
        type            zeroGradient;
    }
    hx
    {
        type            zeroGradient;
    }
}


